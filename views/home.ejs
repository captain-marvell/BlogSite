
<%- include('partials/header.ejs'); -%>
<h1>Home</h1>

<p><%= contents %></p>

 <% posts.forEach(function(post) { %>
 <h1><%= post.title %></h1>
 <p><%= post.content.substring(0, 100) + " ..." %>
<a href="/posts/<%=post.title%>">Read More</a>
 </p>
<% }) %>
<button class="btn btn-success" type="button"  name="newpost"><a href="/compose.ejs">Create Post</a></button>
